#include <iostream>
#include <conio.h>
using namespace std;
int main()
{
      int bil = 10;
      do
      {
            cout<<"HI! do-wile tetap ini dijalankan," <<
            "walaupun kondisi / keadaan salah."<<endl;
      }
      while(bil < 5);
        cout<<endl;
        cout<<"keluar. Bukan pernyataan do-while."<<endl;
        _getche();
      return 0;
}
        
